{
  "builds": [
    {
      "src": "main/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "vercel-build.sh",
      "use": "@vercel/build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main/wsgi.py"
    }
  ]
}
